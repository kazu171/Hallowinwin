  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@10.18.3",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "check": "tsc -b --noEmit",
    "types:gen": "supabase gen types typescript --linked > src/types/database.ts",
    "db:link": "supabase link --project-ref ${PROJECT_REF:-$(cat supabase/.temp/project-ref)}",
    "db:start": "supabase start",
    "db:status": "supabase status",
    "db:stop": "supabase stop",
    "db:migrate:status": "supabase migration list",
    "db:migrate:up": "supabase migration up",
    "db:migrate:new": "supabase migration new"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.2",
    "@tanstack/react-query": "^5.87.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.511.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.8.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.0.2",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/node": "^22.15.30",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
